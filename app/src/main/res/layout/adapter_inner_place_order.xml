<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/_6sdp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginBottom="@dimen/_8sdp">


<RelativeLayout android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/rl_date_view"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="@dimen/_50sdp"
        android:background="@drawable/bg_four_side_corner_inner_color_filled"
        android:paddingHorizontal="@dimen/_10sdp"
        android:visibility="visible"
        >

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10-Mon"
            android:textColor="#373B63"
            android:textAlignment="center"
            android:layout_centerInParent="true"
            android:textSize="@dimen/_13sdp"
            android:fontFamily="@font/poppins"
            android:textStyle="bold"
            />
    </RelativeLayout>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card_one"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="@dimen/_80sdp"
        app:cardCornerRadius="@dimen/_21sdp"
        app:strokeColor="#E2E2E2"
        app:strokeWidth="@dimen/_1sdp"
        android:visibility="gone"
        android:backgroundTint="#FFFFFF">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!-- Green Rounded Edge -->
            <View
                android:id="@+id/subtra_o_14"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="match_parent"
                android:background="@drawable/ic_corner_cut"
                android:backgroundTint="#F5A43D"
                android:visibility="gone"
                />

            <LinearLayout
                android:id="@+id/ll_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingStart="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_6sdp"
                android:visibility="gone"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Nikunj Gupta"
                    android:textColor="@color/black"
                    android:fontFamily="@font/poppins"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:maxLength="15"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Finished"
                    android:textColor="#4AEAB1"
                    android:fontFamily="@font/poppins"
                    android:textSize="@dimen/_11sdp"
                    android:layout_marginTop="6dp"
                    android:textStyle="bold"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="01:00-02:00"
                    android:textColor="@color/black"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:fontFamily="@font/poppins"
                    android:textSize="@dimen/_10sdp"
                    />
            </LinearLayout>

        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>




</RelativeLayout>
</androidx.core.widget.NestedScrollView>